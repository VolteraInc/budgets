{
  "name": "my-gas-app",
  "private": true,
  "type": "module",
  "scripts": {
    "gas:build": "cd gas && tsc -p tsconfig.json",
    "gas:copy": "copyfiles -u 1 ./gas/appsscript.json ./gas/dist/",
    "gas:push": "clasp push --force",
    "ui:dev": "cd ui && vite",
    "ui:build": "cd ui && vite build",
    "ui:deploy": "cd ui && gh-pages -d dist -r git@github.com:VolteraInc/budgets.git",
    "push": "npm run ui:build && npm run ui:deploy && npm run gas:build && npm run gas:copy && npm run gas:push"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/google-apps-script": "^2.0.7",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "copyfiles": "^2.4.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
